<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    tools:ignore="HardcodedText" >

    <CheckBox
        style="@style/check_box_style"
        android:id="@+id/maint_last_modified_list_dlg_listview_item_checkbox"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:text="" />

    <TextView
        android:id="@+id/maint_last_modified_list_dlg_listview_item_syncprof"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="SyncProfile"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/maint_last_modified_list_dlg_listview_item_status"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:text="initialyzed"
        android:textAppearance="?android:attr/textAppearanceLarge" />

</LinearLayout>